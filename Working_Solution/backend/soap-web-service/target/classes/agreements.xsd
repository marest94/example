<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://localhost:8080/soap"
           targetNamespace="http://localhost:8080/soap" elementFormDefault="qualified">
    <xs:element name="getAgreementsRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="agreementID" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="getAgreementsResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="agreement" type="tns:agreement"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="agreement">
        <xs:sequence>
            <xs:element name="agreementID" type="xs:int"/>
            <xs:element name="userID" type="xs:int"/>
            <xs:element name="agreementType" type="xs:string"/>
            <xs:element name="date" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:element name="getAgreementsListResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="agreementsList">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="agreement" type="tns:agreementsList" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="agreementsList">
		<xs:sequence>
			<xs:element name="agremeent" type="tns:agreement"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
</xs:schema> 